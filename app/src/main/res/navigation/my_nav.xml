<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/viewAdapterFragment">
    <fragment
        android:id="@+id/viewAdapterFragment"
        android:name="com.example.quitsmoking.fragments.adapters.ViewAdapterFragment"
        android:label="fragment_view_adapter"
        tools:layout="@layout/fragment_view_adapter" >
        <action
            android:id="@+id/action_viewAdapterFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.quitsmoking.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>